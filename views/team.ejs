
<h1> team</h1>
<br>

<div class="row">
    <div class="col-4">
        <div class="list-group" id="list-tab" role="tablist">

<% teamMembers.forEach(member => { %>
            <a class="list-group-item list-group-item-action "  id="list-<%- member.name%>-list" data-toggle="list" href="#<%- member.name%>" role="tab" ><%= member.name %></a>
<% }) %>
        </div>
    </div>
    <div class="col-8">
        <div class="tab-content" id="nav-tabContent">
        <% teamMembers.forEach(member => { %>
            <div class="tab-pane fade show" id="<%- member.name%>" role="tabpanel" >  <%= member.age %> </div>
        <% }) %>
            </div>
        </div>
    </div>
 </div>   


<br>



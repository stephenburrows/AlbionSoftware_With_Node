<div class="row">
    <div class="col-4">
        <div class="list-group" id="list-tab" role="tablist">

        <% for( let index = 0; index < teamMembers.length; index++ ) { %>

<% if (index==0) { %>
    <a class="list-group-item list-group-item-action active  tm-3"  id="list-<%-  teamMembers[index].name%>-list" data-toggle="list" href="#<%- teamMembers[index].name%>" role="tab" ><%= teamMembers[index].name %></a>
<% } else { %>
    <a class="list-group-item list-group-item-action tm-3"  id="list-<%-  teamMembers[index].name%>-list" data-toggle="list" href="#<%-  teamMembers[index].name%>" role="tab" ><%=  teamMembers[index].name %></a>
<% } %> 

<% } %>
        </div>
    </div>
    <div class="col-8">
        <div class="tab-content " id="nav-tabContent ">
        <% for( let index = 0; index < teamMembers.length; index++ ) { %>

<% if (index==0) { %>


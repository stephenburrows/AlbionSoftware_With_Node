<h1> team</h1>
<br>
<div class="container-fluid"> 

<ul class="list-group">
<% teamMembers.forEach(member => { %>
<li class="list-group-item d-flex justify-content-between align-items-center active">
<%= member.name %> 
<span class="badge badge-primary badge-pill"><%= member.age %> </span>
</li>
<% }) %>
</ul>
</div>
